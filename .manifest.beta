{
	"name" : "__MSG_extensionBetaName__",
	"short_name" : "__MSG_extensionBetaShortName__",
	"description" : "__MSG_extensionDescription__",
	"version" : "0.7.3",
	"author" : "thiscouldbejd",
	"default_locale" : "en",
	"manifest_version" : 2,
	"app" : {
		"background" : {
			"scripts" : ["js/background.js"]
		}
	},
	"icons" : {
    "16": "img/icons/beta_icon_16x16.png",
    "48": "img/icons/beta_icon_48x48.png",
    "128": "img/icons/beta_icon_128x128.png"
  },
	"offline_enabled" : true,
	"kiosk_enabled" : true,
	"permissions" : [
		{"fileSystem" : ["write", "directory"]},
		"storage",
    "power"
	],
  "storage" : {
		"managed_schema" : "schema.json"
	},
	"file_handlers" : {
		"text" : {
			"types" : [
				"text/*"
			]
		}
	}
}